@model   IEnumerable<Models.Student> 

@{
    
    ViewBag.Title = "StudentIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<ul>
    @if(Model!=null)
{
    foreach (var item in Model)
	{
        <li><span>姓名：@item.name</span><span>性别：@item.gender</span><span>年龄：@item.age</span></li>
	}
}
</ul>